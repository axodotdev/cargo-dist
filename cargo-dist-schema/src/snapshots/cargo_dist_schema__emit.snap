        {
          "description": "Homebrew (usually for Mac)",
          "type": "string",
          "enum": [
            "homebrew"
          ]
        },
        {
          "description": "Apt (Debian, Ubuntu, etc)",
          "type": "string",
          "enum": [
            "apt"
          ]
        }
      ]
    },
    "PrRunMode": {
      "description": "Type of job to run on pull request",
