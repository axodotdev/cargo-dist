    "Release": {
      "description": "A Release of an Application",
      "type": "object",
      "required": [
        "app_name",
        "app_version"
      ],
      "properties": {
        "app_name": {
          "description": "The name of the app",
          "type": "string"
        },
        "app_version": {
          "description": "The version of the app",
          "type": "string"
        },
        "artifacts": {
          "description": "The artifacts for this release (zips, debuginfo, metadata...)",
