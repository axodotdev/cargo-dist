        "major": {
          "description": "Major version",
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "series": {
          "description": "Series (minor) version",
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    },
    "Hosting": {
      "description": "Possible hosting providers",
      "type": "object",
      "properties": {
        "github": {
          "description": "Hosted on Github Releases",
