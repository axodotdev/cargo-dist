---
source: cargo-dist-schema/src/lib.rs
expression: "serde_json::to_string_pretty(&schema).unwrap()"
---
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DistManifest",
  "description": "The final report of cargo-dist",
  "type": "object",
  "required": [
    "releases"
  ],
  "properties": {
    "releases": {
      "description": "App releases we're distributing",
      "type": "array",
      "items": {
        "$ref": "#/definitions/Release"
      }
    }
  },
  "definitions": {
    "Artifact": {
      "description": "A distributable bundle that's part of a Release\n\ni.e. a zip or installer",
      "type": "object",
      "oneOf": [
        {
          "description": "A zip or a tarball",
          "type": "object",
          "required": [
            "kind"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "executable-zip"
              ]
            }
          }
        },
        {
          "description": "Standalone Symbols/Debuginfo for a build",
          "type": "object",
          "required": [
            "kind"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "symbols"
              ]
            }
          }
        },
        {
          "description": "Machine-readable metadata",
          "type": "object",
          "required": [
            "kind"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "dist-metadata"
              ]
            }
          }
        }
      ],
      "required": [
        "assets",
        "name"
      ],
      "properties": {
        "assets": {
          "description": "Assets included in the bundle (like executables)",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Asset"
          }
        },
        "name": {
          "description": "The unique name of the artifact (e.g. `myapp-v1.0.0-x86_64-pc-windows-msvc.zip`)",
          "type": "string"
        },
        "path": {
          "description": "The location of the artifact on the local system",
          "type": [
            "string",
            "null"
          ]
        },
        "target_triple": {
          "description": "The target triple of the bundle",
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Asset": {
      "description": "An asset contained in an artifact (executable, license, etc.)",
      "type": "object",
      "oneOf": [
        {
          "description": "An executable artifact",
          "type": "object",
          "required": [
            "kind"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "executable"
              ]
            },
            "symbols_artifact": {
              "description": "The name of the Artifact containing symbols for this executable",
              "type": [
                "string",
                "null"
              ]
            }
          }
        },
        {
          "description": "A README file",
          "type": "object",
          "required": [
            "kind"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "readme"
              ]
            }
          }
        },
        {
          "description": "A LICENSE file",
          "type": "object",
          "required": [
            "kind"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "license"
              ]
            }
          }
        }
      ],
      "required": [
        "name",
        "path"
      ],
      "properties": {
        "name": {
          "description": "The high-level name of the asset",
          "type": "string"
        },
        "path": {
          "description": "The path of the asset relative to the root of the artifact",
          "type": "string"
        }
      }
    },
    "Release": {
      "type": "object",
      "required": [
        "app_name",
        "app_version",
        "artifacts"
      ],
      "properties": {
        "app_name": {
          "description": "The name of the app",
          "type": "string"
        },
        "app_version": {
          "description": "The version of the app",
          "type": "string"
        },
        "artifacts": {
          "description": "The artifacts for this release (zips, debuginfo, metadata...)",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Artifact"
          }
        }
      }
    }
  }
}
