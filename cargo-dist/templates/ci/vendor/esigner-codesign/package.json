{
  "name": "esigner-codesign",
  "version": "1.3.0",
  "private": true,
  "description": "SSL.com CodeSignTool Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run package && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SSLcom/esigner-codesign.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "sslcom",
    "codesigner"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "3.2.4",
    "@actions/core": "1.10.1",
    "@actions/tool-cache": "2.0.1",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "@types/semver": "7.5.7",
    "@typescript-eslint/parser": "6.0.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.44.0",
    "eslint-plugin-github": "4.8.0",
    "eslint-plugin-jest": "27.2.2",
    "jest": "29.6.1",
    "js-yaml": "4.1.0",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  }
}
